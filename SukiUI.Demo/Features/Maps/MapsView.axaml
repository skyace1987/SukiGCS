<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:maps="clr-namespace:SukiGCS.Maps;assembly=SukiGCS.Maps"
             xmlns:local="clr-namespace:SukiUI.Demo.Features.Maps"
             xmlns:avalonia="clr-namespace:Material.Icons.Avalonia;assembly=Material.Icons.Avalonia"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:DataType="local:MapsViewModel"
             x:Class="SukiUI.Demo.Features.Maps.MapsView">
    <Grid>
        <!-- 主地图 -->
        <maps:MapControl Token="{Binding Token}"
                        Background="{DynamicResource SukiBackground}"
                        HorizontalAlignment="Stretch"
                        VerticalAlignment="Stretch" />
    </Grid>
</UserControl> 